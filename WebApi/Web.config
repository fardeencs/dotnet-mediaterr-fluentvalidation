  <elmah>
    <!--<errorLog type="Elmah.XmlFileErrorLog, Elmah" logPath="~/App_Data" />-->
    <!--<errorMail from="fromEmail@xxx.com" to="toEmail@xxx.com" priority="high" subject="Application ELMAH Error" smtpPort="25" smtpServer="mySMTP.xxx.com"/>-->
